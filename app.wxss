.wish-container {
    width: 100%;
    height: 100vh;
    background-size: 100% 100%;
    position: relative;
}
.wish-inner-container{
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    width:100%;
    height: 100%;
}
.wish-inputBtn-bar{
    width: 480rpx;
    height: 100rpx;
    border-radius: 15rpx;
    background: #fff;
    text-align: center;
    font-size: 36rpx;
    color: #333;
    font-family: 'Microsoft YaHei UI';
    line-height: 100rpx;
    margin: 333rpx auto 0 auto;
}